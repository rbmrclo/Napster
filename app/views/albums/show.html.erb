	<ul class="nav nav-pills">
  	<li><%= link_to "Home", root_path %></li>
  	<li><%= link_to "Artists", artists_path %></li>
  	<li class = "active"><%= link_to "Albums", artist_album_path(1,1) %></li>
	</ul>
	<div class ="dialog-album">
		<h1>All Albums </h1><br/>
	<% @album.each do |album| %>
	 	<h3><%= link_to album.album_name.titlecase , artist_album_track_path(album.artist_id, album.id) %>
	 	</h3>
	 	Artist: <span class = "muted"><%x = Artist.find(album.artist_id,:select =>"name")%> 
	 	<%= x.name.titlecase %></span> | 

	 	Tracks: <span class="muted"><%= album.tracks.count.to_s() %></span>
	<% end %>
	</div>